-- FORMS
  [form:0] = HUMAN FORM
  [form:1] = BEAR FORM
  [form:2] = AQUATIC FORM
  [form:3] = CAT FORM 
  [form:4] = TRAVEL FORM
  [form:5] = BOOMKIN FORM

-- BASHES
#showtooltip
/cast [form:0] Cat Form(Shapeshift); [form:1] Bash; [form:2] Cat Form(Shapeshift); [form:3] Pounce; [form:4] Cat Form(Shapeshift); [form:5] Cat Form(Shapeshift); [form:6] ;


